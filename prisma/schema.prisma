generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model OTP {
  id             Int      @id @default(autoincrement())
  otp            String
  expiration     DateTime
  userIdentifier String   
  createdAt      DateTime @default(now())

  @@map("otps")
}
